COLUMN TABLE "IBP_USA_PLAN_DAY"(
	"CUSTID" VARCHAR(10),
	"LOCID" VARCHAR(4),
	"PRDID" VARCHAR(18),
	"DAY" VARCHAR(10),
	"WEEK" VARCHAR(10),
	"MONTH" VARCHAR(10),
	"CURRID" VARCHAR(5),
	"SENSEDDEMANDQTY" DECIMAL(18, 6),
	"ADJSENSEDDEMANDQTY" DECIMAL(18, 6),
	"FINALSENSEDDEMANDQTY" DECIMAL(18, 6),
	"DELIVQTY" DECIMAL(18, 6),
	"ZBILLING" DECIMAL(18, 6),
	PRIMARY KEY (
		"CUSTID",
		"LOCID",
		"PRDID",
		"DAY",
		"WEEK",
		"MONTH"
	)
) UNLOAD PRIORITY 5 AUTO MERGE;